{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-netfilx"
		},
		"data_form_web_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'data_form_web'"
		},
		"ds_sink_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ds_sink'"
		},
		"ds_validation_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ds_validation'"
		},
		"HttpServer_github_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com"
		},
		"data_form_web_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://scnetflixprojectdatalake.dfs.core.windows.net/"
		},
		"ds_sink_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://scnetflixprojectdatalake.dfs.core.windows.net/"
		},
		"ds_validation_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://scnetflixprojectdatalake.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-netfilx_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"data_form_web_accountKey": {
						"value": "[parameters('data_form_web_accountKey')]"
					},
					"ds_sink_accountKey": {
						"value": "[parameters('ds_sink_accountKey')]"
					},
					"ds_validation_accountKey": {
						"value": "[parameters('ds_validation_accountKey')]"
					},
					"HttpServer_github_properties_typeProperties_url": {
						"value": "[parameters('HttpServer_github_properties_typeProperties_url')]"
					},
					"data_form_web_properties_typeProperties_url": {
						"value": "[parameters('data_form_web_properties_typeProperties_url')]"
					},
					"ds_sink_properties_typeProperties_url": {
						"value": "[parameters('ds_sink_properties_typeProperties_url')]"
					},
					"ds_validation_properties_typeProperties_url": {
						"value": "[parameters('ds_validation_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}